pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               // echo 'Hello World'
               git branch: 'main', url: 'https://github.com/nanbod/ncpl_project_1.git'
            }
        }
        stage('Deploy') {
            steps {
              sh "cp -f * /var/www/html"
            }
        }
    }
}
